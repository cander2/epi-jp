      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:ID">
            <xs:enumeration value="HDR_AddresseeOfLiteratureRequest"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="heading" type="xs:string" fixed="fixing" use="required"/>
    </xs:complexType>
  </xs:element>

  <!-- 文献請求先及び問い合わせ先の情報 -->
  <xs:element name="AddresseeInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Detail"/>
        <xs:element minOccurs="0" ref="AddresseeOfInquiry"/>
        <xs:element minOccurs="0" ref="Address"/>
        <xs:element minOccurs="0" ref="ContactInformation"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 旧SGMLの文献請求先及び問い合わせ先の情報 -->
  <xs:element name="Obsolete-SGML-AddresseeInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Obsolete-SGML-PhoneNumber"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-FaxNumber"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 問い合わせ先 -->
  <xs:element name="AddresseeOfInquiry" type="cdata.content-TYPE"/>

  <!-- 住所 -->
  <xs:element name="Address" type="cdata.content-TYPE"/>

  <!-- 連絡先 -->
  <xs:element name="ContactInformation" type="VariousForms-TYPE"/>

  <!-- 旧SGMLの電話番号 -->
  <xs:element name="Obsolete-SGML-PhoneNumber" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLのFAX番号 -->
  <xs:element name="Obsolete-SGML-FaxNumber" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- ２５.保険給付上の注意 -->
  <xs:element name="AttentionOfInsurance">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="ContentOfAttentionOfInsurance"/>
      </xs:sequence>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:ID">
            <xs:enumeration value="HDR_AttentionOfInsurance"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="heading" type="xs:string" fixed="fixing" use="required"/>
    </xs:complexType>
  </xs:element>

  <!-- 保険給付上の注意 -->
  <xs:element name="ContentOfAttentionOfInsurance" type="VariousForms-TYPE"/>

  <!-- ２６.製造販売業者等 -->
  <xs:element name="NameAddressManufact">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="Manufacturer"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Obsolete-SGML-Manufacturer"/>
      </xs:sequence>
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:ID">
            <xs:enumeration value="HDR_NameAddressManufact"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="heading" type="xs:string" fixed="fixing" use="required"/>
    </xs:complexType>
  </xs:element>

  <!-- 製造業者 -->
  <xs:element name="Manufacturer">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TypeOfIndustry"/>
        <xs:element ref="Name"/>
        <xs:element minOccurs="0" ref="Address"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 旧SGMLの製造業者 -->
  <xs:element name="Obsolete-SGML-Manufacturer">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Obsolete-SGML-TypeOfIndustry"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-PhoneNumber"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-FaxNumber"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 製造販売業者等の氏名又は名称及び住所の業種別 -->
  <xs:element name="TypeOfIndustry" type="cdata.content-TYPE"/>

  <!-- 名称 -->
  <xs:element name="Name" type="cdata.content-TYPE"/>

  <!-- 旧SGMLの製造販売業者等の氏名又は名称及び住所の業種別 -->
  <xs:element name="Obsolete-SGML-TypeOfIndustry" type="cdata.content-TYPE"/>

  <!-- （参考情報） -->
  <xs:element name="ReferenceInformation" type="VariousForms-TYPE"/>


<!-- ***************文字レベルの要素モデル******************** -->

  <!-- データ型：cdata.content-TYPE（末端要素のテキスト内モデル） -->
  <xs:complexType name="cdata.content-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="Lang"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
    <!-- 今回・前回改訂箇所を含むことを表す属性 -->
    <xs:attribute name="modified" type="xs:IDREFS"/>
  </xs:complexType>

  <!-- データ型：cdata.contentWithIDrequired-TYPE（ID必須の末端要素のテキスト内モデル） -->
  <xs:complexType name="cdata.contentWithIDrequired-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="Lang"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID" use="required"/>
    <!-- 今回・前回改訂箇所を含むことを表す属性 -->
    <xs:attribute name="modified" type="xs:IDREFS"/>
  </xs:complexType>

  <!-- データ型：cdata.contentWithoutID-TYPE（末端要素のテキスト内モデル） -->
  <xs:complexType name="cdata.contentWithoutID-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="Lang"/>
    </xs:sequence>
    <!-- 今回・前回改訂箇所を含むことを表す属性 -->
    <xs:attribute name="modified" type="xs:IDREFS"/>
  </xs:complexType>

  <!-- データ型：cdata.contentBase-TYPE "cdata.content○○○-TYPE"の基底の要素モデル -->
  <xs:complexType name="cdata.contentBase-TYPE" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="Italic"/>
      <xs:element ref="Sup"/>
      <xs:element ref="Sub"/>
      <xs:element ref="InlineGraphic"/>
      <xs:element ref="ApprovalBrandNameRef"/>
      <xs:element ref="ReferenceBookRef"/>
      <xs:element ref="EmbeddingText"/>
      <xs:element ref="CommentRef"/>
      <xs:element ref="Comment"/>
      <xs:element ref="HeaderRef"/>
      <xs:element ref="Link"/>
    </xs:choice>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <!-- 文字列の言語指定用要素 -->
  <xs:element name="Lang">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBase-TYPE">
          <xs:attribute ref="xml:lang" use="required"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- データ型：cdata.contentBaseWithXMLLANGoptional-TYPE（cdata.contentBase-TYPEから派生して、xml:langをオプションとして追加した） -->
  <xs:complexType name="cdata.contentBaseWithXMLLANGoptional-TYPE" mixed="true">
    <xs:complexContent>
      <xs:extension base="cdata.contentBase-TYPE">
        <xs:attribute ref="xml:lang"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- 一文字単位での文字スタイル -->

  <!-- イタリック -->
  <xs:element name="Italic" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 上付き文字 -->
  <xs:element name="Sup" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 下付き文字 -->
  <xs:element name="Sub" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- インライン・グラフィック -->
  <xs:element name="InlineGraphic">
    <xs:complexType>
      <!-- グラフィックデータファイル名  -->
      <xs:attribute name="gfname" type="xs:string" use="required"/>
      <!-- 縮尺指定（%） -->
      <xs:attribute name="scale" type="xs:nonNegativeInteger" use="optional"/>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 販売名参照 -->
  <xs:element name="ApprovalBrandNameRef">
    <xs:complexType>
      <!-- DetailBrandName（販売名毎の明細）への参照 -->
      <xs:attribute name="ref" type="xs:IDREF"/>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 文献参照 -->
  <xs:element name="ReferenceBookRef">
    <xs:complexType>
      <!-- Reference（文献）への参照 -->
      <xs:attribute name="ref" type="xs:IDREF" use="required"/>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- テキスト埋め込み -->
  <xs:element name="EmbeddingText">
    <xs:complexType>
      <!-- 主にCompositionAndProperty（組成・性状）内の要素への参照 （※主にTblBlockForCompositionAndProperty（組成・性状の表）の中で使われる） -->
      <xs:attribute name="ref" type="xs:IDREF" use="required"/>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 注釈参照 -->
  <xs:element name="CommentRef">
    <xs:complexType>
      <!-- Comment（注釈コメント）への参照 -->
      <xs:attribute name="ref" type="xs:IDREF" use="required"/>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 注釈コメント -->
  <xs:element name="Comment" type="cdata.contentWithIDrequired-TYPE"/>

  <!-- 見出し参照 -->
  <xs:element name="HeaderRef">
    <xs:complexType>
      <!-- 項目要素への参照 -->
      <xs:attribute name="ref" type="xs:IDREF" use="required"/>
      <xs:attribute name="id" type="xs:ID"/>
      <!-- コメント -->
      <xs:attribute name="remarks" type="xs:string"/>
    </xs:complexType>
  </xs:element>

  <!-- リンク(アンカータグ) -->
  <xs:element name="Link">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBase-TYPE">
          <xs:attribute name="url" type="xs:string"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>


  <!-- ***************段落レベルの要素モデル******************** -->

  <!-- データ型：Header-Detail-List-TYPE（見出しと説明の対のモデル) -->
  <xs:complexType name="Header-Detail-List-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="Item"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="List-Contents-TYPE">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Item"/>
      </xs:choice>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <!-- データ型：VariousForms-TYPE（汎用的な段落モデル) -->
  <xs:complexType name="VariousForms-TYPE">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Detail"/>
        <xs:element ref="OrderedList"/>
        <xs:element ref="UnorderedList"/>
        <xs:element ref="SimpleList"/>
        <xs:element ref="TblBlock"/>
        <xs:element ref="Graphic"/>
      </xs:choice>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <!-- データ型：VariousFormsWithIDrequired-TYPE（ID必須の汎用的な段落モデル) -->
  <xs:complexType name="VariousFormsWithIDrequired-TYPE">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Detail"/>
        <xs:element ref="OrderedList"/>
        <xs:element ref="UnorderedList"/>
        <xs:element ref="SimpleList"/>
        <xs:element ref="TblBlock"/>
        <xs:element ref="Graphic"/>
      </xs:choice>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID" use="required"/>
  </xs:complexType>

  <!-- データ型：VariousFormsWithoutID-TYPE（汎用的な段落モデル) -->
  <xs:complexType name="VariousFormsWithoutID-TYPE">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Detail"/>
        <xs:element ref="OrderedList"/>
        <xs:element ref="UnorderedList"/>
        <xs:element ref="SimpleList"/>
        <xs:element ref="TblBlock"/>
        <xs:element ref="Graphic"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <!-- 項目 -->
  <xs:element name="Item">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Header"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded" >
          <xs:element ref="Detail"/>
          <xs:element ref="OrderedList"/>
          <xs:element ref="UnorderedList"/>
          <xs:element ref="SimpleList"/>
          <xs:element ref="TblBlock"/>
          <xs:element ref="Graphic"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="heading" type="xs:string" fixed="free"/>
    </xs:complexType>
  </xs:element>

  <!-- 項目の中身（データ） -->
  <xs:element name="Detail" type="cdata.content-TYPE"/>

  <!-- 旧SGMLの項目の中身（データ） -->
  <xs:element name="Obsolete-SGML-Detail" type="cdata.content-TYPE"/>

  <!-- 見出し -->
  <xs:element name="Header" type="cdata.content-TYPE"/>

  <!-- 旧SGMLの見出し -->
  <xs:element name="Obsolete-SGML-Header" type="cdata.content-TYPE"/>

  <!-- 順序付きリスト -->
  <xs:element name="OrderedList">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="List-Contents-TYPE">
          <xs:attribute name="numberContinued" type="xs:boolean" default="false"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 順序なしリスト -->
  <xs:element name="UnorderedList" type="List-Contents-TYPE"/>

  <!-- 単純リスト -->
  <xs:element name="SimpleList" type="List-Contents-TYPE"/>

  <!-- グラフィック -->
  <xs:element name="Graphic">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="GraphicCaption"/>
        <xs:element ref="GraphicBody"/>
        <xs:element minOccurs="0" ref="GraphicCaption"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- グラフィック本体 -->
  <xs:element name="GraphicBody">
    <xs:complexType>
      <!-- グラフィックデータファイル名  -->
      <xs:attribute name="gfname" type="xs:string" use="required"/>
      <!-- 縮尺指定（%） -->
      <xs:attribute name="scale" type="xs:nonNegativeInteger" use="optional"/>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- グラフィック見出し -->
  <xs:element name="GraphicCaption" type="cdata.content-TYPE"/>

  <!-- データ型：Repeated-Details-TYPE（Detailの繰り返し) -->
  <xs:complexType name="Repeated-Details-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="Detail"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <!-- データ型：Repeated-Details-Max2-TYPE（Detailの繰り返し(2つまで)) -->
  <xs:complexType name="Repeated-Details-Max2-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="2" ref="Detail"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <!-- データ型：TblBlocks-TYPE（TblBlockの繰り返し) -->
  <xs:complexType name="TblBlocks-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="TblBlock"/>
    </xs:sequence>
  </xs:complexType>


  <!-- *********************** 旧SGMLのadvblkandlows ********************** -->

  <xs:complexType name="Obsolete-SGML-AdvblkAndLows-TYPE">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="Obsolete-SGML-AdvblkItem"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <!-- 旧SGMLの副作用等発現状況の概要 -->
  <xs:element name="Obsolete-SGML-AdvblkItem">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Obsolete-SGML-Header"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-Frequency"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded" >
          <xs:element ref="Obsolete-SGML-Detail"/>
          <xs:element ref="OrderedList"/>
          <xs:element ref="UnorderedList"/>
          <xs:element ref="SimpleList"/>
          <xs:element ref="TblBlock"/>
          <xs:element ref="Graphic"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 旧SGMLの頻度 -->
  <xs:element name="Obsolete-SGML-Frequency" type="cdata.content-TYPE"/>

  <!-- *********************** table subset ********************** -->

  <!-- 表全体 -->
  <xs:element name="TblBlock">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="TblCaption"/>
          <xs:element minOccurs="0" ref="WidthDefinition"/>
          <xs:element minOccurs="0" ref="SimpTblHead"/>
          <xs:element ref="SimpleTable"/>
          <xs:element minOccurs="0" ref="SimpTblFoot"/>
          <xs:element minOccurs="0" ref="TblCaption"/>
        </xs:sequence>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 表の見出し -->
  <xs:element name="TblCaption">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.content-TYPE">
          <!-- 表示位置(上部/下部) -->
          <xs:attribute name="align" default="top">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="top"/>
                <xs:enumeration value="bottom"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 表の幅定義 -->
  <xs:element name="WidthDefinition">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Col"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <!-- 表の全体幅指定  -->
      <xs:attribute name="totalWidth" type="xs:string"/>
    </xs:complexType>
  </xs:element>

  <!-- 表の各列の幅定義 -->
  <xs:element name="Col">
    <xs:complexType>
      <xs:attribute name="id" type="xs:ID"/>
      <!-- 列の幅指定  -->
      <xs:attribute name="width" type="xs:string"/>
    </xs:complexType>
  </xs:element>

  <!-- 表のヘッダー -->
  <xs:element name="SimpTblHead">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Detail"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 表の本体 -->
  <xs:element name="SimpleTable">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="SimpTblRow"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 表の行 -->
  <xs:element name="SimpTblRow">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="SimpTblCell"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 表のセル -->
  <xs:element name="SimpTblCell">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="VariousForms-TYPE">
          <!-- 縦方向のセル結合 -->
          <xs:attribute name="rspan" type="xs:nonNegativeInteger"/>
          <!-- 横方向のセル結合 -->
          <xs:attribute name="cspan" type="xs:nonNegativeInteger"/>
          <!-- 横方向の揃え -->
          <xs:attribute name="align" default="left">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="left"/>
                <xs:enumeration value="right"/>
                <xs:enumeration value="center"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <!-- 縦方向の揃え -->
          <xs:attribute name="valign" default="middle">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="top"/>
                <xs:enumeration value="middle"/>
                <xs:enumeration value="bottom"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 表のフッター -->
  <xs:element name="SimpTblFoot">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Detail"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <!-- 横方向の揃え -->
      <xs:attribute name="align" default="left">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="left"/>
            <xs:enumeration value="right"/>
            <xs:enumeration value="center"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
</xs:schema>
