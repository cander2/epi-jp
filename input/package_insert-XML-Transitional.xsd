<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://info.pmda.go.jp/namespace/prescription_drugs/package_insert/1.0/Transitional"
           xmlns="http://info.pmda.go.jp/namespace/prescription_drugs/package_insert/1.0/Transitional">

  <!--xml:langを宣言したスキーマ文書をインポート-->
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="PMDA-xml-lang.xsd"/>

  <!-- 添付文書 -->
  <xs:element name="PackIns">
    <xs:complexType>
      <xs:sequence>
        <!--添付文書番号-->
        <xs:element ref="PackageInsertNo"/>
        <!--企業コード-->
        <xs:element ref="CompanyIdentifier"/>
        <!-- ア.作成又は改訂年月 -->
        <xs:element ref="DateOfPreparationOrRevision"/>
        <!-- イ.日本標準商品分類番号 -->
        <xs:element ref="Sccj"/>
        <!--旧SGMLの日本標準商品分類番号等 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-SccjEtc"/>
        <!-- オ.薬効分類名 -->
        <xs:element minOccurs="0" ref="TherapeuticClassification"/>
        <!-- 承認等 -->
        <xs:element minOccurs="0" ref="ApprovalEtc"/>
        <!-- キ.一般的名称 -->
        <xs:element ref="GenericName"/>
        <!-- ワクチン株の補足情報 -->
        <xs:element minOccurs="0" ref="SupplementaryInformaitonOfVaccineStrain"/>
        <!-- 特殊記載項目（※  当該欄は行政が指示した内容のみ記載すること） -->
        <xs:element minOccurs="0" ref="SpeciallyDescribedItems"/>
        <!-- １.警告 -->
        <xs:element minOccurs="0" ref="Warnings"/>
        <!-- ２.禁忌（次の患者には投与しないこと） -->
        <xs:element minOccurs="0" ref="ContraIndications"/>
        <!--旧SGMLの原則禁忌 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-AvoidedAdministration"/>
        <!-- ３.組成・性状-->
        <xs:element minOccurs="0" ref="CompositionAndProperty"/>
        <!-- ４.効能又は効果-->
        <xs:element minOccurs="0" ref="IndicationsOrEfficacy"/>
        <!-- 旧SGMLの効能又は効果のデータ型：VariousForms-TYPE以外の部分の退避用 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-IndicationsOrEfficacy"/>
        <!-- ５.効能又は効果に関連する注意 -->
        <xs:element minOccurs="0" ref="EfficacyRelatedPrecautions"/>
        <!-- ６.用法及び用量 -->
        <xs:element minOccurs="0" ref="InfoDoseAdmin"/>
        <!-- ７.用法及び用量に関連する注意 -->
        <xs:element minOccurs="0" ref="InfoPrecautionsDosage"/>
        <!-- ８.重要な基本的注意 -->
        <xs:element minOccurs="0" ref="ImportantPrecautions"/>
        <!--旧SGMLの重要な基本的注意の表 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-TblForImportantPrecautions"/>
        <!-- ９.特定の背景を有する患者に関する注意 -->
        <xs:element minOccurs="0" ref="UseInSpecificPopulations"/>
        <!-- １０.相互作用 -->
        <xs:element minOccurs="0" ref="Interactions"/>
        <!-- １１.副作用 -->
        <xs:element minOccurs="0" ref="AdverseEvents"/>
        <!-- １２.臨床検査結果に及ぼす影響 -->
        <xs:element minOccurs="0" ref="InfluenceOnLaboratoryValues"/>
        <!-- １３.過量投与 -->
        <xs:element minOccurs="0" ref="OverDosage"/>
        <!-- １４.適用上の注意 -->
        <xs:element minOccurs="0" ref="PrecautionsForApplication"/>
        <!-- １５.その他の注意 -->
        <xs:element minOccurs="0" ref="OtherPrecautions"/>
        <!--旧SGMLの「その他」 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-Others"/>
        <!-- １６.薬物動態 -->
        <xs:element minOccurs="0" ref="Pharmacokinetics"/>
        <!--旧SGMLの薬物動態の表の退避用 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-TblForPharmacokinetics"/>
        <!-- １７.臨床成績 -->
        <xs:element minOccurs="0" ref="ResultsOfClinicalTrials"/>
        <!--旧SGMLの臨床成績の表の退避用 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-TblForResultsOfClinicalTrials"/>
        <!-- １８.薬効薬理 -->
        <xs:element minOccurs="0" ref="EfficacyPharmacology"/>
        <!--旧SGMLの薬効薬理の表の退避用 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-TblForEfficacyPharmacology"/>
        <!-- １９.有効成分に関する理化学的知見 -->
        <xs:element minOccurs="0" ref="PhyschemOfActIngredients"/>
        <!--旧SGMLの有効成分に関する理化学的知見の表の退避用 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-TblForPhyschemOfActIngredients"/>
        <!-- ２０.取扱い上の注意 -->
        <xs:element minOccurs="0" ref="PrecautionsForHandling"/>
        <!-- ２１.承認条件 -->
        <xs:element minOccurs="0" ref="ConditionsOfApproval"/>
        <!-- ２２.包装 -->
        <xs:element minOccurs="0" ref="Package"/>
        <!-- ２３.主要文献 -->
        <xs:element minOccurs="0" ref="MainLiterature"/>
        <!-- ２４.文献請求先及び問い合わせ先 -->
        <xs:element minOccurs="0" ref="AddresseeOfLiteratureRequest"/>
        <!-- ２５.保険給付上の注意 -->
        <xs:element minOccurs="0" ref="AttentionOfInsurance"/>
        <!-- ２６.製造販売業者等 -->
        <xs:element ref="NameAddressManufact"/>
        <!-- （参考情報） -->
        <xs:element minOccurs="0" ref="ReferenceInformation"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <!--スキーマ・バージョン-->
      <xs:attribute name="version" type="xs:string" use="required"/>
      <!--XML文書種別-->
      <xs:attribute name="xmlKind" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <!-- 添付文書 -->
            <xs:enumeration value="Packins"/>
            <!-- 使用上の注意の照合元データ -->
            <xs:enumeration value="ReferenceOfPrecautionsForUse"/>
            <!-- 取扱い上の注意の照合元データ -->
            <xs:enumeration value="ReferenceOfPrecautionsForHandling"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <!--添付文書形式-->
      <xs:attribute name="drugType">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <!-- 医療用医薬品 -->
            <xs:enumeration value="Medicine"/>
            <!-- ワクチン類及びトキソイド類 -->
            <xs:enumeration value="Vaccine"/>
            <!-- 抗毒素及び検査に用いる生物学的製剤 -->
            <xs:enumeration value="Antitoxin"/>
            <!-- 全血製剤、血液成分製剤 -->
            <xs:enumeration value="BloodProduct"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <!--使用上の注意の照合元データID-->
      <xs:attribute name="referenceOfPrecautionsForUse" type="xs:string"/>
      <!--取扱い上の注意の照合元データID-->
      <xs:attribute name="referenceOfPrecautionsForHandling" type="xs:string"/>
      <!-- 対応言語 -->
      <xs:attribute name="containedLang" type="xs:NMTOKENS"/>
    </xs:complexType>
  </xs:element>

  <!-- 添付文書番号 -->
  <xs:element name="PackageInsertNo" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 企業コード -->
  <xs:element name="CompanyIdentifier" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- ア.作成又は改訂年月 -->
  <xs:element name="DateOfPreparationOrRevision">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="2" ref="PreparationOrRevision"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 改訂情報 -->
  <xs:element name="PreparationOrRevision">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="YearMonth"/>
        <xs:element minOccurs="0" ref="Version"/>
        <xs:element minOccurs="0" ref="ReasonForRevision"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-YearMonth"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-Version"/>
      </xs:sequence>
      <!-- 改訂回識別子 -->
      <xs:attribute name="id" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:ID">
            <xs:enumeration value="今回"/>
            <xs:enumeration value="前回"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="created" type="xs:integer"/>
    </xs:complexType>
  </xs:element>

  <!-- 年月 -->
  <xs:element name="YearMonth">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="Date-gYearMonth">
          <xs:attribute name="id" type="xs:ID"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>

  <!-- [型定義]：「年月日」型あるいは「年月」型のいずれか -->
  <xs:simpleType name="Date-gYearMonth">
    <xs:union memberTypes="xs:date gYearMonth-Null"/>
  </xs:simpleType>

  <!-- 版数 -->
  <xs:element name="Version" type="cdata.content-TYPE"/>

  <!-- 改訂理由 -->
  <xs:element name="ReasonForRevision" type="cdata.content-TYPE"/>

  <!-- 旧SGMLの日付情報 -->
  <xs:element name="Obsolete-SGML-YearMonth" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの版数情報 -->
  <xs:element name="Obsolete-SGML-Version" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- イ.日本標準商品分類番号 -->
  <xs:element name="Sccj">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SccjNo"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 日本標準商品分類番号 -->
  <xs:element name="SccjNo" >
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBaseWithXMLLANGoptional-TYPE">
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 旧SGMLの日本標準商品分類番号等 -->
  <xs:element name="Obsolete-SGML-SccjEtc">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Obsolete-SGML-YrMoOfReexamination"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-DetailOfReexamination"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-YrMoOfReevaluation"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-DetailOfReevaluation"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-YrMoOfAdditionalApproval"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-DetailOfAdditionalApproval"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-InternationalBirthday"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-DetailOfInternationalBirthday"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 旧SGMLの再審査結果公表年月（最新） -->
  <xs:element name="Obsolete-SGML-YrMoOfReexamination" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの再審査結果公表年月（最新）の注意書き -->
  <xs:element name="Obsolete-SGML-DetailOfReexamination" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの再評価結果公表年月（最新） -->
  <xs:element name="Obsolete-SGML-YrMoOfReevaluation" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの再評価結果公表年月（最新）の注意書き -->
  <xs:element name="Obsolete-SGML-DetailOfReevaluation" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの効能又は効果追加承認年月（最新） -->
  <xs:element name="Obsolete-SGML-YrMoOfAdditionalApproval" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの効能又は効果追加承認年月（最新） -->
  <xs:element name="Obsolete-SGML-DetailOfAdditionalApproval" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの国際誕生年月 -->
  <xs:element name="Obsolete-SGML-InternationalBirthday" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- 旧SGMLの国際誕生年月の注意書き -->
  <xs:element name="Obsolete-SGML-DetailOfInternationalBirthday" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>


  <!-- オ.薬効分類名 -->
  <xs:element name="TherapeuticClassification" type="Repeated-Details-TYPE"/>

  <!-- 承認等 -->
  <xs:element name="ApprovalEtc">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DetailBrandName"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 販売名毎の明細 -->
  <xs:element name="DetailBrandName">
    <xs:complexType>
      <xs:sequence>
        <!-- 販売名 -->
        <xs:element ref="ApprovalBrandName"/>
        <!-- 販売名コード -->
        <xs:element ref="BrandCode"/>
        <!-- 販売名英字表記 -->
        <xs:element minOccurs="0" ref="TrademarkInEnglish"/>
        <!-- 販売名ひらがな -->
        <xs:element minOccurs="0" ref="BrandNameInHiragana"/>
        <!-- ウ.承認番号 -->
        <xs:element minOccurs="0" ref="ApprovalAndLicenseNo"/>
        <!-- ウ.販売開始年月 -->
        <xs:element minOccurs="0" ref="StartingDateOfMarketing"/>
        <!-- 旧SGMLの販売開始年月 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-StartingDateOfMarketing"/>
        <!-- エ.貯法、有効期間 -->
        <xs:element minOccurs="0" ref="Storage"/>
        <!-- キ.基準名 -->
        <xs:element minOccurs="0" ref="StandardName"/>
        <!-- 旧SGMLの基準名 -->
        <xs:element minOccurs="0" ref="Obsolete-SGML-StandardName"/>
        <!-- カ.規制区分 -->
        <xs:element minOccurs="0" ref="RegulatoryClassification"/>
        <!-- 製品の補足情報 -->
        <xs:element minOccurs="0" ref="SupplementaryInformationOfProduct"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 販売名 -->
  <xs:element name="ApprovalBrandName" type="cdata.content-TYPE"/>

  <!-- 販売名コード -->
  <xs:element name="BrandCode">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="YJCode"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Code"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- ＹＪ（医情研）コード -->
  <xs:element name="YJCode">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBaseWithXMLLANGoptional-TYPE">
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 一般コード -->
  <xs:element name="Code">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CodeName"/>
        <xs:element minOccurs="0" ref="CodeVersion"/>
        <xs:element ref="CodeValue"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- コード名称 -->
  <xs:element name="CodeName" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- コード体系のバージョン -->
  <xs:element name="CodeVersion">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="id" type="xs:ID"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>

  <!-- コード値 -->
  <xs:element name="CodeValue">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="id" type="xs:ID"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>

  <!-- 販売名英字表記 -->
  <xs:element name="TrademarkInEnglish">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TrademarkName"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 個別販売名英字表記 -->
  <xs:element name="TrademarkName">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBaseWithXMLLANGoptional-TYPE">
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 販売名ひらがな -->
  <xs:element name="BrandNameInHiragana">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="NameInHiragana"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- ひらがな名称 -->
  <xs:element name="NameInHiragana">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBaseWithXMLLANGoptional-TYPE">
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- ウ.承認番号 -->
  <xs:element name="ApprovalAndLicenseNo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="ApprovalNo"/>
        <xs:element minOccurs="0" ref="LicenseNo"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 承認番号 -->
  <xs:element name="ApprovalNo">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBaseWithXMLLANGoptional-TYPE">
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- 許可番号 -->
  <xs:element name="LicenseNo">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cdata.contentBaseWithXMLLANGoptional-TYPE">
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!-- ウ.販売開始年月 -->
  <xs:element name="StartingDateOfMarketing">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="gYearMonth-Null">
          <xs:attribute name="id" type="xs:ID"/>
          <xs:attribute name="modified" type="xs:IDREFS"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>

  <!-- [型定義]：「年月」型（NULL値も許容） -->
  <xs:simpleType name="gYearMonth-Null">
    <xs:union memberTypes="xs:gYearMonth">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value=""/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <!-- 旧SGMLの販売開始年月 -->
  <xs:element name="Obsolete-SGML-StartingDateOfMarketing">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Obsolete-SGML-YearMonth"/>
        <xs:element minOccurs="0" ref="Obsolete-SGML-OtherDescription"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- 旧SGMLのその他の記述 -->
  <xs:element name="Obsolete-SGML-OtherDescription" type="cdata.contentBaseWithXMLLANGoptional-TYPE"/>

  <!-- エ.貯法、有効期間 -->
  <xs:element name="Storage">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="StorageMethod"/>
        <xs:element minOccurs="0" ref="ShelfLife"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="OtherInformation"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <!-- 貯法 -->
  <xs:element name="StorageMethod" type="cdata.content-TYPE"/>

  <!-- 有効期間 -->
  <xs:element name="ShelfLife" type="cdata.content-TYPE"/>

  <!-- その他の項目 -->
  <xs:element name="OtherInformation">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="Header"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="Detail"/>
          <xs:element ref="OrderedList"/>
          <xs:element ref="UnorderedList"/>
          <xs:element ref="SimpleList"/>
          <xs:element ref="TblBlock"/>
          <xs:element ref="Graphic"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="heading" type="xs:string" fixed="free" use="required"/>
    </xs:complexType>
  </xs:element>

  <!-- キ.基準名 -->
  <xs:element name="StandardName">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="4" ref="StandardNameCategory"/>
        <xs:element minOccurs="0" ref="OtherStandardName"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="modified" type="xs:IDREFS"/>
    </xs:complexType>
  </xs:element>

  <!-- 基準名種別コードと脚注を参照する添え字 -->
  <xs:element name="StandardNameCategory">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="StandardNameCategoryCode"/>
        <xs:element ref="StandardNameDetail"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- 基準名種別コード -->
  <xs:element name="StandardNameCategoryCode">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="4"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <!-- 基準名詳細 -->
  <xs:element name="StandardNameDetail" type="cdata.content-TYPE"/>

  <!-- その他基準名 -->
  <xs:element name="OtherStandardName">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="OtherStandardNameCategory"/>
        <xs:element ref="StandardNameDetail"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- その他基準名種別 -->
  <xs:element name="OtherStandardNameCategory" type="cdata.content-TYPE"/>

  <!-- 旧SGMLの基準名 -->
